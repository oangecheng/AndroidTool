<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="orange.com.androidtool">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".View.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <!-- 注册Activity -->
        <activity android:name=".View.DatabaseActivity"/>
        <activity android:name=".View.MainActivity"/>
        <activity android:name=".View.ListActivity">
        </activity>
        <activity android:name=".View.ItemActivity">
        </activity>
        <activity android:name=".View.MyTabActivity">
        </activity>
        <activity android:name=".View.SessionActivity">
        </activity>
        <activity android:name=".View.ContacterActivity">
        </activity>
        <activity android:name=".View.SettingActivity">
        </activity>
        <activity android:name=".View.PhotoActivity">
        </activity>
    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest>